{
    "name": "sonata-project/entity-audit-bundle",
    "type": "symfony-bundle",
    "description": "Audit for Doctrine Entities",
    "keywords": [
        "Persistence",
        "Database",
        "Audit"
    ],
    "license": "LGPL-2.1",
    "require": {
        "php": "^7.3 || ^8.0",
        "doctrine/dbal": "^2.5",
        "doctrine/orm": "^2.7",
        "symfony/config": "^4.4 || ^5.2"
    },
    "conflict": {
        "doctrine/dbal": "2.7.0",
        "doctrine/doctrine-bundle": "<1.4",
        "gedmo/doctrine-extensions": "<2.3.1",
        "symfony/framework-bundle": "<2.7"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.4 || ^2.2",
        "gedmo/doctrine-extensions": "^3.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2.1",
        "symfony/framework-bundle": "^4.4 || ^5.2",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/var-dumper": "^4.4 || ^5.2"
    },
    "autoload": {
        "psr-4": {
            "SimpleThings\\EntityAudit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleThings\\EntityAudit\\Tests\\": "tests"
        }
    }
}
