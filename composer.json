{
    "name": "sonata-project/entity-audit-bundle",
    "type": "symfony-bundle",
    "description": "Audit for Doctrine Entities",
    "keywords": [
        "Persistence",
        "Database",
        "Audit"
    ],
    "license": "LGPL-2.1",
    "require": {
        "php": "^7.3 || ^8.0",
        "doctrine/collections": "^1.6",
        "doctrine/common": "^3.1",
        "doctrine/dbal": "^2.13 || ^3.0",
        "doctrine/event-manager": "^1.1",
        "doctrine/orm": "^2.10",
        "doctrine/persistence": "^2.2",
        "symfony/config": "^4.4 || ^5.3 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.3 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.3 || ^6.0",
        "symfony/security-core": "^4.4 || ^5.3 || ^6.0",
        "twig/twig": "^2.6 || ^3.0"
    },
    "conflict": {
        "doctrine/doctrine-bundle": "<1.4",
        "gedmo/doctrine-extensions": "<2.3.1",
        "symfony/framework-bundle": "<4.4"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.4 || ^2.2",
        "gedmo/doctrine-extensions": "^3.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.93",
        "phpstan/phpstan-doctrine": "^0.12.44",
        "phpstan/phpstan-phpunit": "^0.12.22",
        "phpstan/phpstan-symfony": "^0.12.44",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16.1",
        "psalm/plugin-symfony": "^3.0",
        "symfony/cache": "^4.4 || ^5.3 || ^6.0",
        "symfony/http-foundation": "^4.4 || ^5.3 || ^6.0",
        "symfony/phpunit-bridge": "^5.3 || ^6.0",
        "symfony/var-dumper": "^4.4 || ^5.3 || ^6.0",
        "vimeo/psalm": "^4.8",
        "weirdan/doctrine-psalm-plugin": "^1.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SimpleThings\\EntityAudit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleThings\\EntityAudit\\Tests\\": "tests"
        }
    }
}
